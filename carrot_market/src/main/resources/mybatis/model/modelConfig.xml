<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
	<typeAliases>
		<!-- Member Model -->
		<typeAlias
			type="com.mycompany.carrotMarket.member.vo.MemberVO" alias="memberVO" />

		<!-- Article Model -->
		<typeAlias
			type="com.mycompany.carrotMarket.article.vo.ArticleVO"
			alias="articleVO" />
		<typeAlias
			type="com.mycompany.carrotMarket.article.vo.ImageVO" alias="imageVO" />
		<typeAlias
			type="com.mycompany.carrotMarket.article.vo.LikeVO" alias="likeVO" />

		<!-- Chat Model -->
		<typeAlias type="com.mycompany.carrotMarket.chat.vo.ChatVO"
			alias="chatVO" />
		<typeAlias
			type="com.mycompany.carrotMarket.chat.vo.MessageVO" alias="messageVO" />

		<!-- Trade Model -->
		<typeAlias
			type="com.mycompany.carrotMarket.trade.vo.TradeVO" alias="tradeVO" />

		<!-- Review Model -->
		<typeAlias
			type="com.mycompany.carrotMarket.review.vo.ReviewVO" alias="reviewVO" />

	</typeAliases>
</configuration>